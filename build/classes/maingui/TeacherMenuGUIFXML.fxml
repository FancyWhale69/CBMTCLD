<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="maingui.TeacherMenuGUIController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="20.0" styleClass="mainFxmlClass" stylesheets="@mainguifxml.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#postChoise" prefHeight="40.0" prefWidth="400.0" stylesheets="@ButtonStyle.css" text="Post" textFill="#247ba0" translateX="100.0">
               <font>
                  <Font name="Calibri Bold" size="21.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#Option" prefHeight="40.0" prefWidth="400.0" stylesheets="@ButtonStyle.css" text="Options" textFill="#247ba0" translateX="100.0">
               <font>
                  <Font name="Calibri Bold" size="21.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#Signout" stylesheets="@BackButtonStyle.css" text="Sign Out" translateY="90.0">
               <font>
                  <Font name="Calibri Bold" size="21.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets top="150.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
